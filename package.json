{
  "name": "photoboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "forever": "node_modules/forever/bin/forever start ./bin/www",
    "kill": "node_modules/forever/bin/forever stop 0",
    "restart": "node_modules/forever/bin/forever restart ./bin/www",
    "logs": "node_modules/forever/bin/forever logs",
    "tail": "bin/tail-logs.sh"
  },
  "dependencies": {
    "express": "~4.0.0",
    "static-favicon": "~1.0.0",
    "morgan": "~1.0.0",
    "cookie-parser": "~1.0.1",
    "body-parser": "~1.0.0",
    "debug": "~0.7.4",
    "jade": "~1.3.0",
    "multiparty": "~3.2.4",
    "nedb": "~0.10.5",
    "forever": "0.11.1",
    "mysql": "2.3.0",
    "log-timestamp": "0.1.1"
  }
}
